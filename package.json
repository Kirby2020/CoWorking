  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kirby2020/CoWorking/issues"
  },
  "homepage": "https://github.com/Kirby2020/CoWorking#readme",
  "description": "A school project to make a multiplayer game using electron.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kirby2020/CoWorking.git"
  },
  "keywords": [
    "electron",
    "plants",
    "versus",
    "zombies",
    "multiplayer"
  ],
  "main": "./client/js/main.js",
  "scripts": {
    "startServer": "node ./server/server.js",
